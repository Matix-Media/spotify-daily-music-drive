{
    "compilerOptions": {
        "lib": ["es2021"],
        "module": "commonjs",
        "target": "es2021",
        "strictNullChecks": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "sourceMap": true
    }
}
